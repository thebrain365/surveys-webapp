generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model PERSON {
  ID          String       @id @default(uuid())
  FULL_NAME   String
  DOB         DateTime
  CONTACT     String       @unique
  PREFERENCES PREFERENCE[]
  FOODS       FOOD[]
}

model PREFERENCE {
  ID     String   @id @default(uuid())
  NAME   String   @unique
  PEOPLE PERSON[]
}

model FOOD {
  ID     String   @id @default(uuid())
  NAME   String   @unique
  PEOPLE PERSON[]
}
